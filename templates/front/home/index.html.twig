{% extends 'base.html.twig' %}

{% block title %}
  Front
{% endblock %}
   {% block javascripts %}
      {{ encore_entry_script_tags('match') }}
    {% endblock %}

{% block body %}
{% if app.user %}


<style>
    .example-wrapper { margin: 8em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .mw { max-width: 850px;}
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style> 
<div class=" my-20 mx-auto mw">
   <div class="flex font-sans lg:flex">
  <div class="margin-auto flex-none w-[20rem] relative z-[10] card">
